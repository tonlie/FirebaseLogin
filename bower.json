  "homepage": "https://github.com/tonlie/seed-element/",
  "ignore": [
    "/.*",
    "/test/"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.2.0",
    "paper-menu-button": "PolymerElements/paper-menu-button#~1.0.4",
    "firebase-element": "GoogleWebComponents/firebase-element#~1.0.9",
    "paper-icon-button": "PolymerElements/paper-icon-button#~1.0.6"
  },
  "devDependencies": {
    "iron-component-page": "PolymerElements/iron-component-page#^1.0.0",
    "web-component-tester": "^3.4.0"
  }
